{
  "name": "paulvl/backup",
  "description": "Database backup package for laravel",
  "license": "MIT",
  "authors": [
    {
      "name": "Paul Vidal",
      "email": "paul@tecactus.com"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=5.5.9",
    "illuminate/console": "5.0.* || 5.1.* || 5.2.* || 5.3.* || 5.4.* || 5.5.* || 5.6.* || 5.7.* || 5.8.* || 6.* || 7.* || 8.* || 9.*",
    "illuminate/support": "5.0.* || 5.1.* || 5.2.* || 5.3.* || 5.4.* || 5.5.* || 5.6.* || 5.7.* || 5.8.* || 6.* || 7.* || 8.* || 9.*",
    "league/flysystem-aws-s3-v3": "~1.0",
    "ext-zlib": "*"
  },
  "autoload": {
    "psr-4": {
      "Backup\\": "src/"
    },
    "files": [
      "src/Support/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Backup\\BackupServiceProvider"
      ]
    }
  }
}
